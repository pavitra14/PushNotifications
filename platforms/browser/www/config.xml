<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="8" id="com.pyaarekdhokahai.app" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Pyaar ek Dhoka hai</name>
    <description>
        Pyaar Ek Dhoka hai Push Notifications app.
    </description>
    <author email="me@pbehre.in" href="http://pbehre.in">
        Pavitra Behre
    </author>
    <content src="index.html" />
    <allow-navigation href="*" />
    <access origin="*" subdomains="true" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <hook src="scripts/incrementBuildNum.js" type="after_build" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="19" />
</widget>
